pipeline {

  agent any

  stages {
    stage(clean) {
      steps {
        withMaven(maven: 'Maven 3.6', jdk: 'JDK 11') {
          bat 'mvn clean'
        }
      }
    }

    stage(install) {
      steps {
        withMaven(maven: 'Maven 3.6', jdk: 'JDK 11') {
          bat 'mvn install -DskipTests'
        }
      }
    }

  }
}
