pipeline {

  agent any

  stages {
    stage(CLEAN) {
      steps {
        withMaven(maven: 'Maven 3.6', jdk: 'JDK 11') {
          mvn clean
        }
      }
    }

    stage(INSTALL) {
      steps {
        withMaven(maven: 'Maven 3.6', jdk: 'JDK 11') {
          mvn install -DskipTests
        }
      }
    }

  }
}
