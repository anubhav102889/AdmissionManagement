pipeline {

  agent any

  stages {
    stage(CLEAN) {
      steps {
        withMaven(maven: 'Maven 3.6', jdk: 'JDK 11') {
          bat 'mvn clean'
        }
      }
    }

    stage(INSTALL) {
      steps {
        withMaven(maven: 'Maven 3.6', jdk: 'JDK 11') {
          bat 'mvn install -DskipTests'
        }
      }
    }

  }
}
